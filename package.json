{
  "name": "mprimo-backend",
  "version": "1.0.0",
  "description": "E-commerce marketplace backend",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp swagger-output.json dist/swagger-output.json",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=integration",
    "seed:users": "ts-node src/scripts/seed-users.ts",
    "seed:vendors": "ts-node src/scripts/seed-vendors.ts",
    "seed:products": "ts-node src/scripts/seed-products.ts",
    "seed:faqs": "ts-node src/scripts/seed-faqs.ts",
    "seed:categories": "ts-node src/scripts/seed-categories.ts",
    "seed:interactions": "ts-node src/scripts/seed-interactions.ts",
    "create-superadmin": "ts-node src/scripts/create-superadmin.ts",
    "seed-countries": "ts-node src/scripts/seed-countries.ts",
    "swagger": "node swagger-docs.js",
    "swagger:serve": "swagger-ui-serve src/swagger-output.json",
    "seed:all": "npm run seed:users && npm run seed:products && npm run seed:interactions",
    "clear-analytics": "ts-node src/scripts/clear-analytics.ts",
    "create-subscription-plans": "ts-node src/scripts/create-subscription-plans.ts",
    "create-payment-options": "ts-node src/scripts/create-payment-options.ts"
  },
  "dependencies": {
    "@types/supertest": "^6.0.3",
    "axios": "^1.10.0",
    "axios-cookiejar-support": "^6.0.4",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.40.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "libphonenumber-js": "^1.12.18",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "node-schedule": "^2.1.1",
    "nodemailer": "^7.0.4",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "qrcode": "^1.5.4",
    "redis": "^4.6.7",
    "slugify": "^1.6.6",
    "socket.io": "^4.7.2",
    "stripe": "^18.3.0",
    "supertest": "^7.1.3",
    "tough-cookie": "^5.1.2",
    "web-push": "^3.6.7",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.2",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongodb": "^4.0.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "@types/node-cron": "^3.0.8",
    "@types/node-schedule": "^2.1.7",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-apple": "^2.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/web-push": "^3.6.4",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.5.0",
    "nodemon": "^3.0.1",
    "speakeasy": "^2.0.0",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/tests/**"
    ]
  }
}
